      "enum": [
        "draw",
        "shape-file",
        "address-lookup",
        "address-manual"
      ],
      "type": "string"
    },
    "Commercial": {
      "const": "commercial",
      "type": "string"
    },
    "CommunityGroup": {
      "const": "communityGroup",
      "type": "string"
    },
    "Contact": {
      "$id": "contact",
      "anyOf": [
        {
          "additionalProperties": false,
          "properties": {
            "company": {
              "additionalProperties": false,
              "properties": {
                "name": {
                  "type": "string"
                }
              },
              "required": [
                "name"
              ],
              "type": "object"
            },
            "email": {
              "$ref": "#/definitions/Email"
            },
            "name": {
              "additionalProperties": false,
              "properties": {
                "first": {
                  "type": "string"
                },
                "last": {
                  "type": "string"
                },
                "title": {
                  "type": "string"
                }
              },
              "required": [
                "first",
                "last"
              ],
              "type": "object"
            },
            "phone": {
              "additionalProperties": false,
              "properties": {
                "primary": {
                  "type": "string"
                }
              },
              "required": [
                "primary"
