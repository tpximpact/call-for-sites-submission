        "responses": {
          "anyOf": [
            {
              "items": {
                "$ref": "#/definitions/Response"
              },
              "type": "array"
            },
            {
              "type": "string"
            }
          ]
        }
      },
      "required": [
        "question",
        "responses"
      ],
      "type": "object"
    },
