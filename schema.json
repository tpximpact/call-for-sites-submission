            },
            "neighbouringUseStructuresAffectDevelopmentDetails": {
              "type": "string"
            },
            "notableAbnormalCosts": {
              "type": "boolean"
            },
            "notableAbnormalCostsDetails": {
              "type": "string"
            },
            "potentialScale": {
              "type": "string"
            },
            "potentialUse": {
              "items": {
                "$ref": "#/definitions/SiteUses"
              },
              "type": "array"
            },
            "preApplicationReference": {
              "type": "string"
            },
            "previousAssessmentDate": {
              "$ref": "#/definitions/Date"
            },
            "site": {
