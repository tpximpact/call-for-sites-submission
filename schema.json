            }
          },
          "required": [
            "email",
            "name",
            "phone",
            "siteConnection",
            "type"
          ],
          "type": "object"
        },
        {
          "additionalProperties": false,
          "properties": {
            "company": {
              "additionalProperties": false,
              "properties": {
                "name": {
                  "type": "string"
                }
              },
              "required": [
                "name"
              ],
              "type": "object"
            },
            "email": {
              "$ref": "#/definitions/Email"
            },
            "name": {
              "additionalProperties": false,
              "properties": {
                "first": {
                  "type": "string"
                },
                "last": {
                  "type": "string"
                },
                "title": {
                  "type": "string"
                }
              },
              "required": [
                "first",
                "last"
              ],
              "type": "object"
            },
            "phone": {
              "additionalProperties": false,
              "properties": {
                "primary": {
                  "type": "string"
                }
              },
              "required": [
                "primary"
              ],
              "type": "object"
            },
            "siteConnection": {
              "$ref": "#/definitions/Other",
              "description": "What is this contacts connection to the site?"
            },
